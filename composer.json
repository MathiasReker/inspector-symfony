{
    "name": "inspector-apm/inspector-symfony",
    "description": "Real-Time monitoring for Symfony applications.",
    "keywords": ["monitoring", "php", "inspector", "symfony"],
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Valerio Barbera",
            "email": "valerio@inspector.dev"
        }
    ],
    "require": {
		"php": "^7.1.3",
		"inspector-apm/inspector-php": "^3.0",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/event-dispatcher": "^5.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/phpunit-bridge": "^5.0"
    },
	"autoload": {
        "psr-4": {
            "Inspector\\Symfony\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Inspector\\Symfony\\Tests\\": "tests/"
        }
    },
	"minimum-stability": "dev",
    "prefer-stable": true
}
