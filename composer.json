{
    "name": "inspector-apm/inspector-symfony",
    "description": "Real-Time monitoring for Symfony applications.",
    "keywords": ["monitoring", "php", "inspector", "symfony", "bundle"],
    "homepage":    "https://www.inspector.dev",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Valerio Barbera",
            "email": "valerio@inspector.dev"
        }
    ],
    "require": {
		"php": ">=7.2.5",
		"inspector-apm/inspector-php": "^3.6",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/console": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/yaml": "^5.2",
        "symfony/security-bundle": "^5.2"
	},
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.4",
        "phpunit/phpunit": "^9.5",
        "symfony/test-pack": "^1.0"
    },
	"autoload": {
        "psr-4": {
            "Inspector\\Symfony\\Bundle\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Inspector\\Symfony\\Bundle\\Tests\\": "tests/"
        }
    },
	"minimum-stability": "dev",
    "prefer-stable": true
}
