<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Inspector\Symfony\Bundle\Listeners\KernelEventsSubscriber" public="false">
            <argument type="service" id="inspector"/>
            <tag name="kernel.event_subscriber" />
        </service>
        <service id="Inspector\Symfony\Bundle\Listeners\ConsoleEventsSubscriber" public="false">
            <argument type="service" id="inspector"/>
            <tag name="kernel.event_subscriber" />
        </service>
        <service id="Inspector\Symfony\Bundle\Listeners\DoctrineEventsSubscriber" public="false">
            <argument type="service" id="inspector"/>
            <tag name="doctrine.event_subscriber"/>
        </service>
    </services>
</container>
